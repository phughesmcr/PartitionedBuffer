{
  "typescript.validate.enable": true,
  "editor.defaultFormatter": "denoland.vscode-deno",
  "editor.formatOnSave": true,
  "files.eol": "\n",
  "files.autoSave": "onFocusChange",
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },
  "editor.rulers": [
    120
  ],
  "cSpell.words": [
    "BECSY",
    "behavior",
    "Bitfield",
    "Bitfields",
    "bitflags",
    "Bitpool",
    "CODEOWNERS",
    "codepaths",
    "Deno",
    "devtool",
    "dotfiles",
    "downlevel",
    "ECSY",
    "endregion",
    "esmodules",
    "ESNEXT",
    "flatmap",
    "gamedev",
    "Geotic",
    "HECS",
    "onner",
    "outdir",
    "Parens",
    "partitionedbuffer",
    "PartitionedBuffer",
    "phughesmcr",
    "popcnt",
    "postbuild",
    "sourcemap",
    "stringifier",
    "treeshake",
    "tsbuildinfo",
    "tslib",
    "typechecking"
  ]
}
